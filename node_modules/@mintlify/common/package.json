{
  "name": "@mintlify/common",
  "version": "1.0.1",
  "description": "Commonly shared code within Mintlify",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc --project tsconfig.build.json",
    "clean:build": "rimraf dist",
    "clean:all": "rimraf node_modules .eslintcache && yarn clean:build",
    "watch": "tsc --watch",
    "test": "vitest run",
    "type": "tsc --noEmit",
    "lint": "eslint . --cache",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "author": "Mintlify, Inc.",
  "license": "ISC",
  "devDependencies": {
    "@mintlify/eslint-config": "^1.0.4",
    "@mintlify/eslint-config-typescript": "^1.0.9",
    "@mintlify/ts-config": "^2.0.2",
    "@tsconfig/recommended": "1.x",
    "@typescript-eslint/eslint-plugin": "6.x",
    "@typescript-eslint/parser": "6.x",
    "eslint": "8.x",
    "eslint-config-prettier": "8.x",
    "eslint-plugin-unused-imports": "^3.x",
    "openapi-types": "^12.1.3",
    "prettier": "2.x",
    "rimraf": "^5.0.1",
    "typescript": "4.9.5",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@mintlify/models": "0.0.51"
  },
  "gitHead": "999f35530b15583af7280003c55a90fec3967601"
}
